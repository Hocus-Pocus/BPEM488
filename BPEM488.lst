                                      ; - Calculate fuel burn Litres per Hour ("LpH"), Kilometers per Litre ("KmpL") and
                                      ;   Miles per Gallon Imperial ("MpG")
                                      ;*****************************************************************************************
0055E0 7F55E0 MACRO                       FUEL_BURN_CALCS       ; Macro in injcalcsBPEM.s
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Look up the injector flow rate for 2 injectors (CC/Min)                                                                            * (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
0055E0 7F55E0 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (FUEL_BURN_CALCS)
0055E5 7F55E5 CD 08 00                    ldy   #veBins_E   ; Load index register Y with address of first configurable (FUEL_BURN_CALCS)
                                                            ; constant on buffer RAM page 1 (veBins_E)                 (FUEL_BURN_CALCS)
0055E8 7F55E8 EE EA 03 F0                 ldx   $03F0,Y     ; Load Accu X with value in buffer RAM page 1 (offset 1008)($03F0) (FUEL_BURN_CALCS)
                                                            ; ("InjPrFlo_F")                                           (FUEL_BURN_CALCS)
0055EC 7F55EC B7 56                       tfr  X,Y          ; "InjPrFlo_F"-> Accu Y                                    (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Calculate current fuel burn in Litres per Hour: (Injector open time over 1 second (FUEL_BURN_CALCS)
                                      ;   x 60 = Injector open time for 1 minute. Injector open time for 1 minute x injector (FUEL_BURN_CALCS)
                                      ;   flow rate per minute = injector flow for 1 minute. Injector flow for 1 minute x 60 (FUEL_BURN_CALCS)
                                      ;   = injector flow per hour. For integer math:                                  (FUEL_BURN_CALCS)
                                      ;    ((("FDsec"/10)*6)*InjPrFlo_F)/10,000= "LpH"(Litres per hour x 10)           (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
0055EE 7F55EE FC 10 CC                    ldd  FDsec       ; "FDsec"->Accu D (Fuel delivery pulse width total for 1 Sec (mS*10) (FUEL_BURN_CALCS)
0055F1 7F55F1 CE 00 0A                    ldx  #$000A      ; Decimal 10 -> Accu X                                      (FUEL_BURN_CALCS)
0055F4 7F55F4 18 10                       idiv             ;(D)/(X)->Xrem->D "FDsec"/10                                (FUEL_BURN_CALCS)
0055F6 7F55F6 B7 54                       tfr  X,D         ; Result-> Accu D                                           (FUEL_BURN_CALCS)
0055F8 7F55F8 13                          emul             ; (D)*(Y)->Y:D ("FDsec"/10)*"InjPrFlo_F"                    (FUEL_BURN_CALCS)
0055F9 7F55F9 CE 27 10                    ldx  #$2710      ; Decimal 10,000-> Accu X                                   (FUEL_BURN_CALCS)
0055FC 7F55FC 11                              ediv             ;(Y:D)/)X)->Y;Rem->D (("FDsec"/10)*"InjPrFlo_F")/10,000="LpH" (FUEL_BURN_CALCS)
0055FD 7F55FD 7D 10 CA                    sty  LpH         ; Copy result to "LpH" (Litres per hour x 10)               (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Calculate current fuel burn in Kilometers per Litre                          (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
005600 7F5600 FC 10 A2                    ldd  KPH         ; "KPH" -> Accu D                                           (FUEL_BURN_CALCS)
005603 7F5603 CD 00 0A                    ldy  #$000A      ; Decimal 10 -> Accy Y                                      (FUEL_BURN_CALCS)
005606 7F5606 13                          emul             ; (D)*(Y)->Y:D "KPH"*10                                     (FUEL_BURN_CALCS)
005607 7F5607 FE 10 CA                    ldx  LpH         ; "LpH"-> Accu X                                            (FUEL_BURN_CALCS)
00560A 7F560A 11                              ediv             ;(Y:D)/)X)->Y;Rem->D ("KPH"*10)/LpH = KmpL              (FUEL_BURN_CALCS)
00560B 7F560B 7D 10 D4                    sty  KmpL        ; Result -> "KmpL"                                          (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
                                      ; - Convert fuel burn in Kilometers per Litre to Miles per Gallon Imperial       (FUEL_BURN_CALCS)
                                      ;***************************************************************************************** (FUEL_BURN_CALCS)
00560E 7F560E FC 10 D4                    ldd  KmpL        ; "KmpL" -> Accu D                                          (FUEL_BURN_CALCS)
005611 7F5611 CD 00 64                    ldy  #$0064      ; Decimal 100 -> Accy Y                                     (FUEL_BURN_CALCS)
005614 7F5614 13                          emul             ; (D)*(Y)->Y:D "KmpL"*100                                   (FUEL_BURN_CALCS)
005615 7F5615 B7 46                       tfr  D,Y         ; Result Lo word-> Accu Y                                   (FUEL_BURN_CALCS)
005617 7F5617 CC 01 1A                    ldd  #$011A      ; Decimal 282-> Accu D (2.82 is conversion factor)          (FUEL_BURN_CALCS)
00561A 7F561A 13                          emul             ; (D)*(Y)->Y:D ("KmpL"*100)*282                             (FUEL_BURN_CALCS)
00561B 7F561B CE 27 00                    ldx  #$2700      ; Decimal 10,000 -> Accu X                                  (FUEL_BURN_CALCS)
00561E 7F561E 11                              ediv             ;(Y:D)/)X)->Y;Rem->D (("KmpL"*100)*282)/10,000= "MpG"   (FUEL_BURN_CALCS)
00561F 7F561F 7D 10 EA                    sty   MpG        ; Result -> "MpG"                                           (FUEL_BURN_CALCS)
