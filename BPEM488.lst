005E9A 7F5E9A 4C 01 02                        bset  PORTB,ASDRelay  ; Set "ASDRelay" pin on Port B(LED23 board 1 to 28) (FUEL_PUMP_AND_ASD_ON)
005E9D 7F5E9D 20 20                           bra   CASprdOK        ; Branch to CASprdOK:
005E9F 7F5E9F                         StillCranking:
005E9F 7F5E9F 18 0B FF 00 6E              movb #$FF,ECT_PTPSR   ; Load ECT_PTPSR with %11111111
                                                                ; (prescale 256, 5.12us resolution,
                                                                ; max period 335.5ms)
005EA4 7F5EA4 18 0B FF 03 FE              movb #$FF,TIM_PTPSR   ; Load TIM_PTPSR with %11111111
                                                                ; (prescale 256, 5.12us resolution,
                                                                ; max period 335.5ms)
005EA9 7F5EA9 1D 10 E1 04                 bclr engine,run       ; Clear "run" bit of "engine" variable
005EAD 7F5EAD 1C 10 E1 02                 bset engine,crank     ; Set "crank" bit of "engine" variable
005EB1 7F5EB1 1C 10 E2 01                 bset engine2,base512  ; Set the "base512" bit of "engine" bit field
005EB5 7F5EB5 1D 10 E2 02                 bclr engine2,base256  ; Clear the "base256" bit of "engine" bit field
005EB9 7F5EB9 MACRO                           FUEL_PUMP_AND_ASD_ON  ; Energise fuel pump and ASD Relay (macro in gpio_BEEM.s)
                                                            ; Register (pullups enabled BKGD, Port E and Port A        (FUEL_PUMP_AND_ASD_ON)
                                      ;***************************************************************************************** (FUEL_PUMP_AND_ASD_ON)
                                      ; - Energise the Fuel pump relay and the Emergency Shutdown relay on Port B Bit0 and Bit1 (FUEL_PUMP_AND_ASD_ON)
                                      ;***************************************************************************************** (FUEL_PUMP_AND_ASD_ON)
005EB9 7F5EB9 4C 01 01                    bset  PORTB,FuelPump  ; Set "FuelPump" pin on Port B(LED9 board 1 to 28)     (FUEL_PUMP_AND_ASD_ON)
005EBC 7F5EBC 4C 01 02                        bset  PORTB,ASDRelay  ; Set "ASDRelay" pin on Port B(LED23 board 1 to 28) (FUEL_PUMP_AND_ASD_ON)
005EBF 7F5EBF                         CASprdOK:
005EBF 7F5EBF 1C 10 24 01                 bset  ICflgs,RPMcalc  ; Set "RPMcalc" bit of "ICflgs"
005EC3 7F5EC3                         CASDone:
                                      ;******************************************************************************************
                                      ; - Rev counter -
                                      ;   Used to decrement "ASErev" every revolution  (count down counter for ASE taper)
