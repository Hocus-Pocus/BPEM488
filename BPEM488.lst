0063F2 7F63F2 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
0063F5 7F63F5 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
0063F8 7F63F8 19 EE                       leay D,Y          ; Curve row pointer -> Y
0063FA 7F63FA 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
