                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
00627A 7F627A                         ReEntCrvPos:
00627A 7F627A 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
00627D 7F627D 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006283 7F6283 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
006287 7F6287 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
00628A 7F628A FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
00628D 7F628D 19 EE                       leay D,Y          ; Curve row pointer -> Y
00628F 7F628F 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
