0059AF 7F59AF                         RcvPageID:               ; "rxmode" = 2
0059AF 7F59AF 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL"
0059B1 7F59B1 7A 10 0E                    staa  pageID         ; Copy to "pageID"
0059B4 7F59B4 B6 10 0F                    ldaa  txcmnd         ; Load Accu A with value in "txcmnd"
0059B7 7F59B7 81 03                       cmpa  #$03           ; Compare with decimal 3 ("B")
0059B9 7F59B9 18 27 01 EB                 beq   ModeB2         ; If the Z bit of CCR is set, branch to ModeB2:
0059BD 7F59BD 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059C0 7F59C0 0B                          rti                  ; Return from interrupt
0059C1 7F59C1                         RcvOSmsb:                ; "rxmode" = 3
0059C1 7F59C1 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset MSB)
0059C3 7F59C3 7A 10 0A                    staa  rxoffsetMSB    ; Copy to "rxoffsetMSB"
0059C6 7F59C6 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059C9 7F59C9 0B                          rti                  ; Return from interrupt
0059CA 7F59CA                         RcvOSlsb:                ; "rxmode" = 4
0059CA 7F59CA 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset LSB)
0059CC 7F59CC 7A 10 0B                    staa  rxoffsetLSB    ; Copy to "rxoffsetLSB"
0059CF 7F59CF 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059D2 7F59D2 0B                          rti                  ; Return from interrupt
0059D3 7F59D3                         RcvCntmsb:               ; "rxmode" = 5
0059D3 7F59D3 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count MSB)
0059D5 7F59D5 7A 10 06                    staa  txgoalMSB      ; Copy to "txgoalMSB"
0059D8 7F59D8 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059DB 7F59DB 0B                          rti                  ; Return from interrupt
0059DC 7F59DC                         RcvCntlsb:               ; "rxmode" = 6
0059DC 7F59DC 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count LSB)
0059DE 7F59DE 7A 10 07                    staa  txgoalLSB      ; Copy to "txgoalLSB"
0059E1 7F59E1 B6 10 0F                    ldaa  txcmnd         ; Load Accu A with value in "txcmnd"
0059E4 7F59E4 81 01                       cmpa  #$01           ; Compare with decimal 1 ("C")
0059E6 7F59E6 18 27 01 3E                 beq   ModeC2         ; If the Z bit of CCR is set, branch to ModeC2:
0059EA 7F59EA 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059ED 7F59ED 0B                          rti                  ; Return from interrupt (ready to receive next byte)
0059EE 7F59EE                         RcvData:                 ; "rxmode" = 7
                                      ;*****************************************************************************************
                                      ; - If we are here we must be in "W" mode and receiving either one or
                                      ;   two bytes, depending on the byte count.
                                      ;*****************************************************************************************
0059EE 7F59EE FC 10 06                    ldd  txgoalMSB      ; Load double accumulator with value in
                                                              ; "txgoalMSB:txgoalLSB"
0059F1 7F59F1 8C 00 02                    cpd  #$0002         ; Compare with decimal 2
0059F4 7F59F4 27 05                       beq  RcvDataMSB     ; If equal branch to RcvDataMSB:
0059F6 7F59F6 8C 00 01                    cpd  #$0001         ; Compare with decimal 1
0059F9 7F59F9 27 09                       beq  RcvDataLSB     ; If equal branch to RcvDataLSB:
0059FB 7F59FB                         RcvDataMSB:
0059FB 7F59FB 96 CF                       ldaa  SCI0DRL       ; Load Accu A with value in "SCI0DRL"(data byte)
0059FD 7F59FD 7A 10 10                    staa  dataMSB       ; Copy to "dataMSB"
005A00 7F5A00 72 10 0C                    inc   rxmode        ; Increment "rxmode"(continue to next mode)
005A03 7F5A03 0B                          rti                 ; Return from subroutine
005A04 7F5A04                         RcvDataLSB:             ; "rxmode" = 8
005A04 7F5A04 D6 CF                       ldab  SCI0DRL       ; Load Accu B with value in "SCI0DRL"(data byte)
005A06 7F5A06 7B 10 11                    stab  dataLSB       ; Copy to "dataLSB"
005A09 7F5A09 B6 10 0E                    ldaa  pageID        ; Load accu A with value in "pageID"
005A0C 7F5A0C 81 01                       cmpa  #$01          ; Compare with decimal 1 (send page 1)
005A0E 7F5A0E 27 0A                       beq   StorePg1      ; If the Z bit of CCR is set, branch to StorePg1:
005A10 7F5A10 81 02                       cmpa  #$02          ; Compare with decimal 2 (send page 2)
005A12 7F5A12 27 4B                       beq   StorePg2      ; If the Z bit of CCR is set, branch to StorePg2:
005A14 7F5A14 81 03                       cmpa  #$03          ; Compare with decimal 3 (send page 3)
005A16 7F5A16 18 27 00 86                 beq   StorePg3      ; If the Z bit of CCR is set, branch to StorePg3:
005A1A 7F5A1A                         StorePg1:
