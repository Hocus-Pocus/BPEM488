004F49 7F4F49 CD 27 10                        ldy   #$2710        ; Load index register Y with decimal 10000 (for integer math) (IGN_CALCS_256)
004F4C 7F4F4C 13                              emul                ;(D)x(Y)=Y:D "DwellFin" * 10,000                     (IGN_CALCS_256)
004F4D 7F4F4D CE 01 00                        ldx   #$100         ; Load index register X decimal 256                  (IGN_CALCS_256)
004F50 7F4F50 11                          ediv                ;(Y:D)/(X)=Y;Rem->D ("DwellFin" * 10,000) / 256 = "DwellFintk" (IGN_CALCS_256)
