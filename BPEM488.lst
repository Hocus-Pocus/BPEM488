006297 7F6297 FE 10 2F                    ldx  CrvRowHi     ; Curve row high boundry value -> X                        (CRV_INTERP)
00629A 7F629A 34                          pshx              ; Save to stack                                            (CRV_INTERP)
00629B 7F629B FE 10 31                    ldx  CrvRowLo     ; Curve row low boundry value -> X                         (CRV_INTERP)
00629E 7F629E 34                          pshx              ; Save to stack                                            (CRV_INTERP)
00629F 7F629F FE 10 33                    ldx  CrvColHi     ; Curve column high boundry value -> X                     (CRV_INTERP)
0062A2 7F62A2 34                          pshx              ; Save to stack                                            (CRV_INTERP)
0062A3 7F62A3 FE 10 35                    ldx  CrvColLo     ; Curve column low boundry value -> X                      (CRV_INTERP)
0062A6 7F62A6 34                          pshx              ; Save to stack                                            (CRV_INTERP)
