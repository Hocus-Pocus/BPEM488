005DF6 7F5DF6 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005DF9 7F5DF9 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005DFB 7F5DFB B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005DFD 7F5DFD CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E00 7F5E00 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
005E01 7F5E01 7C 10 1D                        std   Degx10tk512    ; Copy result to "Degx10tk512"
