                                      ;***************************************************************************************** (CONVERT_ATD0/2D_IPOL)
0046C1 7F46C1 E3 82                                   ADDD  \4        ; (A:B)+(M:M+1)->A:B Add Z1                      (CONVERT_ATD0/2D_IPOL)
                                      ;***************************************************************************************** (CONVERT_ATD0)
                                      ; - Free stack space (result in D)                                               (CONVERT_ATD0)
                                      ;***************************************************************************************** (CONVERT_ATD0)
0046C3 7F46C3 1B 8A                       leas  10,SP    ; Stack pointer -> bottom of stack                            (CONVERT_ATD0)
0046C5 7F46C5 7C 10 78                    std   Mapx10   ; Copy result to "Mapx10" Manifold Absolute Pressure x 10     (CONVERT_ATD0)
                                      ;***************************************************************************************** (CONVERT_ATD0)
