                                      ;*****************************************************************************************
005CA0 7F5CA0 1E 10 24 08 0B              brset ICflgs,Ch2alt,VSS2 ; If "Ch2alt" bit of "ICflgs" is set, branch to "VSS2:"
005CA5 7F5CA5 DC 54                       ldd  ECT_TC2H            ; Load accu D with value in "ECT_TC2H"
005CA7 7F5CA7 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005CAA 7F5CAA 1C 10 24 08                 bset ICflgs,Ch2alt       ; Set "Ch2alt" bit of "ICflgs"
005CAE 7F5CAE 20 10                       bra  ECT2_ISR_Done       ; Branch to "ECT2_ISR_Done:"
005CB0 7F5CB0                         VSS2:
005CB0 7F5CB0 DC 54                       ldd  ECT_TC2H       ; Load accu D with value in "ECT_TC2H"
005CB2 7F5CB2 B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC2H"
005CB5 7F5CB5 7C 10 9E                    std  VSSprd         ; Copy result to "VSSprd"
005CB8 7F5CB8 1D 10 24 08                 bclr ICflgs,Ch2alt  ; Clear "Ch2alt" bit of "ICflgs"
005CBC 7F5CBC 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005CC0 7F5CC0                         ECT2_ISR_Done:
005CC0 7F5CC0 0B                          rti                ; Return from Interrupt
