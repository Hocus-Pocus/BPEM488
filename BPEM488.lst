00634E 7F634E FC 10 29                    ldd  CrvColOfst   ; Offset from the curve page to the curve column -> D      (COL_BOUNDARYS)
006351 7F6351 19 EE                       leay D,Y          ; Curve column pointer -> Y                                (COL_BOUNDARYS)
006353 7F6353 18 05 EE 10 35              movw D,Y,CrvColLo ; Copy to curve column low boundry value for interpolation (COL_BOUNDARYS)
