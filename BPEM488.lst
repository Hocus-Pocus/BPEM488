0062A3 7F62A3 FC 10 29                    ldd  CrvColOfst   ; Offset from the curve page to the curve column -> D      (COL_BOUNDARYS)
0062A6 7F62A6 19 EE                       leay D,Y          ; Curve column pointer -> Y                                (COL_BOUNDARYS)
0062A8 7F62A8 18 05 EE 10 35              movw D,Y,CrvColLo ; Copy to curve column low boundry value for interpolation (COL_BOUNDARYS)
