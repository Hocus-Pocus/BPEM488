005C8B 7F5C8B                         ECT_TC2_ISR:
                                      ;*****************************************************************************************
                                      ; - Get two consecutive rising edge signals for vehicle speed and
                                      ;   calculate the period. KPH calculations are done in the main loop
                                      ;*****************************************************************************************
005C8B 7F5C8B 1E 10 24 08 0B              brset ICflgs,Ch2alt,VSS2 ; If "Ch2alt" bit of "ICflgs" is set, branch to "VSS2:"
005C90 7F5C90 DC 54                       ldd  ECT_TC2H            ; Load accu D with value in "ECT_TC2H"
005C92 7F5C92 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005C95 7F5C95 1C 10 24 08                 bset ICflgs,Ch2alt       ; Set "Ch2alt" bit of "ICflgs"
005C99 7F5C99 20 10                       bra  ECT2_ISR_Done       ; Branch to "ECT2_ISR_Done:"
005C9B 7F5C9B                         VSS2:
005C9B 7F5C9B DC 54                       ldd  ECT_TC2H       ; Load accu D with value in "ECT_TC2H"
005C9D 7F5C9D B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC2H"
005CA0 7F5CA0 7C 10 9E                    std  VSSprd         ; Copy result to "VSSprd"
005CA3 7F5CA3 1D 10 24 08                 bclr ICflgs,Ch2alt  ; Clear "Ch2alt" bit of "ICflgs"
005CA7 7F5CA7 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005CAB 7F5CAB                         ECT2_ISR_Done:
005CAB 7F5CAB 0B                          rti                ; Return from Interrupt
