                                      ;******************************************************************************************
005E22 7F5E22 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E25 7F5E25 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd256/72)
005E27 7F5E27 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E29 7F5E29 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E2C 7F5E2C 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk256"
005E2D 7F5E2D 7C 10 1F                        std   Degx10tk256    ; Copy result to "Degx10tk256"
