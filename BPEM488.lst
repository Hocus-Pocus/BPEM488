                                      ;*****************************************************************************************
005D94 7F5D94 18 0B FF 00 17                  movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005D99 7F5D99 CD 08 00                    ldy   #veBins_E       ; Load index register Y with address of first configurable
                                                              ; constant on buffer RAM page 1 (vebins)
005D9C 7F5D9C EC EA 03 E6                 ldd   $03E6,Y       ; Load Accu A with value in buffer RAM page 1 offset 998
                                                              ; "Stallcnt" (stall counter)(offset = 998)
005DA0 7F5DA0 7C 10 22                    std  Stallcnt       ; Copy to "Stallcnt" (no crank or stall condition counter)
                                                              ; (1mS increments)
005DA3 7F5DA3 1E 10 24 04 12              brset ICflgs,Ch1_2nd,CAS_2nd ; If "Ch1_2nd" bit of "ICflgs" is set, branch to "CAS_2nd:"
005DA8 7F5DA8 1E 10 24 10 24              brset ICflgs,Ch1_3d,CAS_3d   ; If "Ch1_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
005DAD 7F5DAD DC 52                       ldd   ECT_TC1H               ; Load accu D with value in "ECT_TC1H"
005DAF 7F5DAF 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005DB2 7F5DB2 1C 10 24 04                 bset  ICflgs,Ch1_2nd         ; Set "Ch1_2nd" bit of "ICflgs"
005DB6 7F5DB6 18 20 00 C0                 bra   CASDone                ; Branch to CASDone:
005DBA 7F5DBA                         CAS_2nd:
005DBA 7F5DBA DC 52                       ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
005DBC 7F5DBC 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005DBF 7F5DBF B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005DC2 7F5DC2 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005DC5 7F5DC5 1D 10 24 04                 bclr  ICflgs,Ch1_2nd  ; Clear "Ch1_2nd" bit of "ICflgs"
005DC9 7F5DC9 1C 10 24 10                 bset  ICflgs,Ch1_3d   ; Set "Ch1_3d" bit of "ICflgs"
005DCD 7F5DCD 18 20 00 A9                 bra   CASDone         ; Branch to CASDone:
005DD1 7F5DD1                         CAS_3d:
005DD1 7F5DD1 DC 52                       ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
005DD3 7F5DD3 B3 10 17                    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC1H"
005DD6 7F5DD6 7C 10 1B                    std   CASprd2tk       ; Copy result to "CASprd2tk"
005DD9 7F5DD9 F3 10 19                    addd  CASprd1tk       ; (A:B)+(M:M+1)_->A:B "CASprd2tk" + "CASprd1tk" = "CASprdtk"
005DDC 7F5DDC 1D 10 24 10                 bclr  ICflgs,Ch1_3d   ; Clear "Ch1_3d" bit of "ICflgs"
                                      ;*****************************************************************************************
                                      ; - All calculations that use the Crank Angle Sensor period need to know what the
                                      ;   resolution is. The timers are initalized with a 5.12uS resoluion but switched to
                                      ;   2.56uS resolution when the engine tranistions from crank mode to run mode.
                                      ;*****************************************************************************************
005DE0 7F5DE0 1E 10 E1 04 17              brset engine,run,CAS256 ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CAS256:
005DE5 7F5DE5 7C 10 9A                    std   CASprd512       ; Copy result to "CASprd512" (CAS period in 5.12uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (5.12uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk512")
                                      ;******************************************************************************************
005DE8 7F5DE8 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005DEB 7F5DEB 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005DED 7F5DED B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005DEF 7F5DEF CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005DF2 7F5DF2 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
005DF3 7F5DF3 7C 10 1D                        std   Degx10tk512    ; Copy result to "Degx10tk512"
005DF6 7F5DF6 18 79 10 1F                     clrw  Degx10tk256    ; Clear "Degx10tk256"
005DFA 7F5DFA 20 15                           bra   CASprdDone     ; Branch to CASprdDone:
005DFC 7F5DFC                         CAS256:
005DFC 7F5DFC 7C 10 9C                    std   CASprd256      ; Copy result to "CASprd256" (CAS period in 2.56uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (2.56uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk256")
                                      ;******************************************************************************************
005DFF 7F5DFF CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E02 7F5E02 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd256/72)
005E04 7F5E04 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E06 7F5E06 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E09 7F5E09 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk256"
005E0A 7F5E0A 7C 10 1F                        std   Degx10tk256    ; Copy result to "Degx10tk256"
005E0D 7F5E0D 18 79 10 1D                 clrw  Degx10tk512    ; Clear "Degx10tk512"
005E11 7F5E11                         CASprdDone:
                                      ;*****************************************************************************************
                                      ; - Determine if the engine is cranking or running. The timer is initialized with a
                                      ;   5.12uS time base and the engine status bit field "engine" is cleared on power up.
                                      ;   "Spantk" will roll over at ~85 RPM with a 5.12uS base and at ~169 RPM with a
                                      ;   2.56uS base. The time base is switched from 5.12uS to 2.56uS at ~300 RPM which should
                                      ;   be at a speed when the engine is running. Engine speed can drop to as low as ~169 RPM
                                      ;   before ignition calculations cannot be done. It is not likely that the engine will
                                      ;   continue to run at this speed and will stall. Stall detection is done in the main
                                      ;   loop if the period between crank sensor signals is greater than ~2 seconds.
                                      ;*****************************************************************************************
005E11 7F5E11 1E 10 E1 04 60              brset  engine,run,CASprdOK ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CASDone:
                                      ;*****************************************************************************************
                                      ; - Look up the cranking RPM limit ("crankingRPM_F")
                                      ;*****************************************************************************************
005E16 7F5E16 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005E1B 7F5E1B CD 08 00                    ldy   #veBins_E   ; Load index register Y with address of first configurable
                                                            ; constant on buffer RAM page 1 (veBins_E)
005E1E 7F5E1E EE EA 03 E2                 ldx   $03E2,Y     ; Load Accu X with value in buffer RAM page 1 (offset 994)($03E2)
                                                            ; ("crankingRPM_F", cranking RPM limit)
                                      ;*****************************************************************************************
                                      ; - Convert to crank angle sensor period ticks in 5.12uS resolution
                                      ;*****************************************************************************************
005E22 7F5E22 CC C3 46                    ldd  #$C346           ; Load accu D with Lo word of  10 cyl RPMk (5.12uS clock tick)
005E25 7F5E25 CD 00 23                    ldy  #$0023           ; Load accu Y with Hi word of 10 cyl RPMk (5.12uS clock tick)
005E28 7F5E28 11                          ediv                  ; Extended divide (Y:D)/(X)=>Y;Rem=>D "RPMk" / "crankingRPM_F"
                                                                ; = crank angle sensor period in 5.12uS resolution
                                      ;*****************************************************************************************
                                      ; - Compare the limit period with the current period
                                      ;*****************************************************************************************
005E29 7F5E29 BD 10 9A                    cpy   CASprd512       ; Compare cranking RPM limit period to "CASprd512"
005E2C 7F5E2C 25 28                           blo   StillCranking   ; Period is greater than that for "crankingRPM_F" so engine is
                                                                    ; still cranking. Branch to StillCranking:
005E2E 7F5E2E 20 00                       bra   SwitchToRun     ; Branch to SwitchToRun:
005E30 7F5E30                         SwitchToRun:
005E30 7F5E30 18 0B 7F 00 6E              movb #$7F,ECT_PTPSR   ; Load ECT_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005E35 7F5E35 18 0B 7F 03 FE              movb #$7F,TIM_PTPSR   ; (TIM_PTPSR equ $03FE) Load TIM_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
