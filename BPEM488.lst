                                      ;***************************************************************************************** (CONVERT_ATD0)
0046C8 7F46C8 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (CONVERT_ATD0)
0046CD 7F46CD CD 08 00                    ldy  #veBins_E   ; Load index register Y with address of first configurable constant (CONVERT_ATD0)
                                                           ; on buffer RAM page 1 (veBins)                             (CONVERT_ATD0)
0046D0 7F46D0 EC EA 03 E8                 ldd  $03E8,Y     ; Load Accu D with value in buffer RAM page 1 offset 1000 (tpsMin) (CONVERT_ATD0)
0046D4 7F46D4 3B                          pshd             ; Push to stack (V1)                                        (CONVERT_ATD0)
0046D5 7F46D5 FC 10 7A                    ldd  tpsADC      ; Load double accumulator with "tpsADCAdc"                  (CONVERT_ATD0)
0046D8 7F46D8 3B                          pshd             ; Push to stack (V)                                         (CONVERT_ATD0)
0046D9 7F46D9 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (CONVERT_ATD0)
0046DE 7F46DE CD 08 00                    ldy  #veBins_E   ; Load index register Y with address of first configurable constant (CONVERT_ATD0)
                                                           ; on buffer RAM page 1 (vebins)                             (CONVERT_ATD0)
0046E1 7F46E1 EC EA 03 EA                 ldd  $03EA,Y     ; Load Accu D with value in buffer RAM page 1 offset 1002 (tpsMax) (CONVERT_ATD0)
0046E5 7F46E5 3B                          pshd             ; Push to stack (V2)                                        (CONVERT_ATD0)
0046E6 7F46E6 CC 00 00                    ldd  #$0000      ; Load double accumulator with decimal 0 (Low range %) ( x 10) (CONVERT_ATD0)
