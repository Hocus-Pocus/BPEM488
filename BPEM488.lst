005DD2 7F5DD2 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005DD5 7F5DD5 1C 10 24 04                 bset  ICflgs,Ch1_2nd         ; Set "Ch1_2nd" bit of "ICflgs"
005DD9 7F5DD9 18 20 00 C6                 bra   CASDone                ; Branch to CASDone:
005DDD 7F5DDD                         CAS_2nd:
005DDD 7F5DDD DC 52                       ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
005DDF 7F5DDF 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005DE2 7F5DE2 B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005DE5 7F5DE5 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005DE8 7F5DE8 1D 10 24 04                 bclr  ICflgs,Ch1_2nd  ; Clear "Ch1_2nd" bit of "ICflgs"
005DEC 7F5DEC 1C 10 24 10                 bset  ICflgs,Ch1_3d   ; Set "Ch1_3d" bit of "ICflgs"
005DF0 7F5DF0 18 20 00 AF                 bra   CASDone         ; Branch to CASDone:
005DF4 7F5DF4                         CAS_3d:
005DF4 7F5DF4 DC 52                       ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
005DF6 7F5DF6 B3 10 17                    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC7H"
005DF9 7F5DF9 7C 10 1B                    std   CASprd2tk       ; Copy result to "CASprd2tk"
