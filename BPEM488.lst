                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
005936 7F5936                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
005936 7F5936                         Inc125uS:
005936 7F5936 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
005939 7F5939 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
00593C 7F593C 81 08                       cmpa #$08          ; Compare it with decimal 8
00593E 7F593E 26 54                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005940 7F5940                         DomS:
005940 7F5940 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
005944 7F5944 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
005947 7F5947 72 10 03                    inc  mS            ; Increment Millisecond counter
00594A 7F594A B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
00594D 7F594D 81 64                       cmpa #$64          ; Compare it with decimal 100
00594F 7F594F 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
005951 7F5951 81 C8                       cmpa #$C8          ; Compare it with decimal 200
005953 7F5953 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
005955 7F5955 81 FA                       cmpa #$FA          ; Compare it with decimal 250
005957 7F5957 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
005959 7F5959 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
00595B 7F595B                         Do100mS:
00595B 7F595B 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
00595F 7F595F 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005961 7F5961                         Do250mS:
005961 7F5961 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
005965 7F5965 79 10 03                    clr  mS            ; Clear Millisecond counter
005968 7F5968 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
00596B 7F596B B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
00596E 7F596E 81 02                       cmpa #$02          ; Compare with decimal 2
005970 7F5970 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
005972 7F5972 81 04                       cmpa #$04          ; Compare with decimal 4
005974 7F5974 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
005976 7F5976 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005978 7F5978                         Do500mS:
005978 7F5978 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
00597C 7F597C B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
00597F 7F597F 81 04                       cmpa #$04          ; Compare with decimal 4
005981 7F5981 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
005983 7F5983 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
005985 7F5985                         DoSec:
005985 7F5985 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
