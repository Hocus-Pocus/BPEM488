004A72 7F4A72 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (ASE_TAPER_LU)
004A77 7F4A77 18 03 08 00 10 25           movw #veBins_E,CrvPgPtr   ; Address of the first value in VE table(in RAM)(page pointer) (ASE_TAPER_LU)
                                                                  ; ->page where the desired curve resides             (ASE_TAPER_LU)
004A7D 7F4A7D 18 03 01 90 10 27           movw #$0190,CrvRowOfst  ; 400 -> Offset from the curve page to the curve row (ASE_TAPER_LU)
                                                                      ; (tempTable2)(actual offset is 800)             (ASE_TAPER_LU)
004A83 7F4A83 18 03 01 CA 10 29           movw #$01CA,CrvColOfst  ; 458 -> Offset from the curve page to the curve column (ASE_TAPER_LU)
                                                                      ; (aseRevTable)(actual offset is 916)            (ASE_TAPER_LU)
004A89 7F4A89 18 04 10 6C 10 2B           movw Cltx10,CrvCmpVal   ; Engine Coolant Temperature (Degrees F x 10) ->     (ASE_TAPER_LU)
                                                                  ; Curve comparison value                             (ASE_TAPER_LU)
004A8F 7F4A8F 18 0B 09 10 2D              movb #$09,CrvBinCnt     ; 9 -> number of bins in the curve row or column minus 1 (ASE_TAPER_LU)
004A94 7F4A94 16 62 EE                    jsr   CRV_LU_NP         ; Jump to subroutine at CRV_LU_NP:(located in        (ASE_TAPER_LU)
                                                                      ; interp_BEEM488.s module)                       (ASE_TAPER_LU)
004A97 7F4A97 7C 10 55                    std   ASErev            ; Copy result to Afterstart Enrichment Taper (revolutions) (ASE_TAPER_LU)
004A9A 7F4A9A 7C 10 A4                    std   ASEcnt            ; Copy result to Afterstart Enrichment Taper counter (ASE_TAPER_LU)
