                                      ;***************************************************************************************** (TOE_OFC_CALCS)
005474 7F5474 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (TOE_OFC_CALCS)
005479 7F5479 CD 08 00                    ldy   #veBins_E   ; Load index register Y with address of first configurable (TOE_OFC_CALCS)
                                                            ; constant on buffer RAM page 1 (veBins_E)                 (TOE_OFC_CALCS)
00547C 7F547C EC EA 03 EC                 ldd   $03EC,Y     ; Load Accu D with value in buffer RAM page 1 (offset 1004)($03EC) (TOE_OFC_CALCS)
                                                            ; ("reqFuel" in Accu B)                                    (TOE_OFC_CALCS)
005480 7F5480 B6 10 BB                    ldaa  TOEpct      ; "TOEpct" -> Accu D (%)                                   (TOE_OFC_CALCS)
005483 7F5483 12                          mul               ;(A)x(B)->A:B (reqFuel" x "TOEpct")                        (TOE_OFC_CALCS)
005484 7F5484 CE 00 64                        ldx  #$0064       ; Decimal 100 -> Accu X                                (TOE_OFC_CALCS)
005487 7F5487 18 10                       idiv              ;(D)/(X)->X:rem->D (reqFuel" x "TOEpct")/10                (TOE_OFC_CALCS)
005489 7F5489 7E 10 BC                    stx  TOEpw        ; Result -> "TOEpw" TOE adder (mS x 10)                    (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
                                      ; - Check to see if Throttle Opening Enrichment is done.                         (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
00548C 7F548C                          TOE_CHK_TIME:                                                                   (TOE_OFC_CALCS)
00548C 7F548C 1E 10 E1 40 07              brset  engine,OFCon,RESET_TOE ; If Overrun Fuel Cut bit of "Engine" bit field is set, (TOE_OFC_CALCS)
                                                                        ; branch to RESET_TOE:                         (TOE_OFC_CALCS)
005491 7F5491 B6 10 CF                     ldaa  TOEdurCnt    ; "TOEdurCnt" -> Accu A                                  (TOE_OFC_CALCS)
005494 7F5494 27 02                            beq   RESET_TOE    ; If "TOEdurCnt" = zero branch to RESET_TOE:(timer has timed out) (TOE_OFC_CALCS)
005496 7F5496 20 1F                            bra   TOE_LOOP     ; Branch to "TOE_LOOP:(Timer hasn't timed out yet)   (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
                                      ; - The throttle is no longer opening and the duration timer has timed out so clear (TOE_OFC_CALCS)
                                      ;    "TOEpct" and the "TOEon" bit of "engine" bit field.                         (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
005498 7F5498                         RESET_TOE:                                                                       (TOE_OFC_CALCS)
