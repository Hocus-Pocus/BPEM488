005AA0 7F5AA0 B6 10 11                    ldaa  dataLSB      ; Load accu A with value in "dataLSB"
005AA3 7F5AA3 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005AA8 7F5AA8 6A E2 08 00                 staa  veBins_E,x     ; Copy "W" data byte to "veBins_E" offset in index register X
005AAC 7F5AAC 18 20 00 80                 bra   StoreDone    ; Branch to StoreDone:
005AB0 7F5AB0                         StorePg2:
