005DCA 7F5DCA 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005DCD 7F5DCD B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005DD0 7F5DD0 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005DD3 7F5DD3 1D 10 24 04                 bclr  ICflgs,Ch1_2nd  ; Clear "Ch1_2nd" bit of "ICflgs"
