                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
00590B 7F590B                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
00590B 7F590B                         Inc125uS:
00590B 7F590B 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
00590E 7F590E B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
005911 7F5911 81 08                       cmpa #$08          ; Compare it with decimal 8
005913 7F5913 26 5A                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005915 7F5915                         DomS:
005915 7F5915 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
005919 7F5919 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
00591C 7F591C 72 10 03                    inc  mS            ; Increment Millisecond counter
00591F 7F591F B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
005922 7F5922 81 64                       cmpa #$64          ; Compare it with decimal 100
005924 7F5924 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
005926 7F5926 81 C8                       cmpa #$C8          ; Compare it with decimal 200
005928 7F5928 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
00592A 7F592A 81 FA                       cmpa #$FA          ; Compare it with decimal 250
00592C 7F592C 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
00592E 7F592E 26 3F                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
005930 7F5930                         Do100mS:
005930 7F5930 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
005934 7F5934 20 39                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005936 7F5936                         Do250mS:
005936 7F5936 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
00593A 7F593A 79 10 03                    clr  mS            ; Clear Millisecond counter
00593D 7F593D 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
005940 7F5940 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
005943 7F5943 81 02                       cmpa #$02          ; Compare with decimal 2
005945 7F5945 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
005947 7F5947 81 04                       cmpa #$04          ; Compare with decimal 4
005949 7F5949 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
00594B 7F594B 20 22                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00594D 7F594D                         Do500mS:
00594D 7F594D 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
005951 7F5951 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
005954 7F5954 81 04                       cmpa #$04          ; Compare with decimal 4
005956 7F5956 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
005958 7F5958 20 15                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
00595A 7F595A                         DoSec:
00595A 7F595A 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
