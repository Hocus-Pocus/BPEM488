                                      ;   using 32x16 divide                                                           (CALC_RPM)
                                      ;***************************************************************************************** (CALC_RPM)
004E50 7F4E50                         RunRPM:                                                                          (CALC_RPM)
004E50 7F4E50 CC 86 8C                    ldd  #$868C         ; Load accu D with Lo word of  10 cyl RPMk (2.56uS clock tick) (CALC_RPM)
004E53 7F4E53 CD 00 47                    ldy  #$0047         ; Load accu Y with Hi word of 10 cyl RPMk (2.56uS clock tick) (CALC_RPM)
004E56 7F4E56 FE 10 9C                    ldx  CASprd256      ; Load "X" register with value in "CASprd256"            (CALC_RPM)
004E59 7F4E59 11                          ediv                ; Extended divide (Y:D)/(X)=>Y;Rem=>D                    (CALC_RPM)
                                                                  ;(Divide "RPMk" by "CASprd256")                      (CALC_RPM)
004E5A 7F4E5A 7D 10 A0                    sty  RPM            ; Copy result to "RPM"                                   (CALC_RPM)
004E5D 7F4E5D 1D 10 24 01                 bclr ICflgs,RPMcalc ; Clear "RPMcalc" bit of "ICflgs"                        (CALC_RPM)
