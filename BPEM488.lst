004E5A 7F4E5A CD 00 23                    ldy  #$0023         ; Load accu Y with Hi word of 10 cyl RPMk (5.12uS clock tick) (CALC_RPM)
004E5D 7F4E5D FE 10 9A                    ldx  CASprd512      ; Load "X" register with value in "CASprd512"            (CALC_RPM)
004E60 7F4E60 11                          ediv                ; Extended divide (Y:D)/(X)=>Y;Rem=>D                    (CALC_RPM)
                                                                  ;(Divide "RPMk" by "CASprd512")                      (CALC_RPM)
004E61 7F4E61 7D 10 A0                    sty  RPM            ; Copy result to "RPM"                                   (CALC_RPM)
004E64 7F4E64 1D 10 24 01                 bclr ICflgs,RPMcalc ; Clear "RPMcalc" bit of "ICflgs"                        (CALC_RPM)
004E68 7F4E68 20 11                           bra  RunRPMDone     ; Branch to RunRPMDone:                              (CALC_RPM)
