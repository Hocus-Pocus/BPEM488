                                      ;*****************************************************************************************
                                      ; - The camshaft position sensor and the crankshaft position sensor are both hall
                                      ;   effect gear tooth sensors. They read notched wheels on their repsective shafts.
                                      ;   When the sensor senses a notch its output pin goes to ground. The BPEM simulator
                                      ;   input from the sensor is the LED circuit of an opto isolator. When the LED in the
                                      ;   opto is powered it biases the output transistor on so the timer channel pin sees a
                                      ;   rising edge, which triggers an interrupt event. The state machine uses these events
                                      ;   to de-code the signals to determine crankshaft position and camshaft phase. Any
                                      ;   event that does not fall into the mechanical order of events triggers an error.
                                      ;   An error will disable ignition and fuel injection until a positive lock on crankshaft
                                      ;   position and camshaft phase is re-established.
                                      ;*****************************************************************************************
005D8D 7F5D8D                         ECT_TC0_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC0_ISR Interrupt Service Routine (Camshaft sensor notch)
                                      ;   Event = 0
                                      ;*****************************************************************************************
005D8D 7F5D8D DE 50                       ldx    ECT_TC0H         ; Read ECT_TC0H to clear the flag
005D8F 7F5D8F CE 6E 9E                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005D92 7F5D92 F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005D95 7F5D95 58                          aslb                    ; Shift Accu B 1 place to the left
005D96 7F5D96 CA 00                       orab   #$00             ; Bit wise inclusive OR Accu B with 0
005D98 7F5D98 A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005D9A 7F5D9A 7A 10 14                    staa   State            ; Copy to "State"
005D9D 7F5D9D 0B                          rti                     ; Return from interrupt
005D9E 7F5D9E                         ECT_TC1_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC1_ISR Interrupt Service Routine (Crankshaft sensor notch)
                                      ;   Event = 1
                                      ;*****************************************************************************************
005D9E 7F5D9E DE 52                       ldx    ECT_TC1H              ; Read ECT_TC1H to clear the flag
005DA0 7F5DA0 CE 6E 9E                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005DA3 7F5DA3 F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005DA6 7F5DA6 58                          aslb                    ; Shift Accu B 1 place to the left
005DA7 7F5DA7 CA 01                       orab   #$01             ; Bit wise inclusive OR Accu B with 1
005DA9 7F5DA9 A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005DAB 7F5DAB 7A 10 14                    staa   State            ; Copy to "State"
005DAE 7F5DAE 81 46                       cmpa    #$46            ; Compare with decimal 70 (Error)
005DB0 7F5DB0 27 06                       beq     State_Error     ; If "State" = $46, branch to State_Error:
005DB2 7F5DB2 81 67                       cmpa    #$67            ; Compare with decimal 103
005DB4 7F5DB4 2F 21                       ble     NoLock          ; If "State" =< $67, branch to NoLock:
005DB6 7F5DB6 2E 11                       bgt     SynchLock       ; If "State" is > $67, branch to Synchlock:
005DB8 7F5DB8                         State_Error:
                                      ;*****************************************************************************************
                                      ; - If we get here we have experienced an unexpected cam or crank input and have lost lock.
                                      ;   No more spark or injection events until lock has been re-established.
                                      ;*****************************************************************************************
005DB8 7F5DB8 79 10 14                    clr   State                     ; Clear "State"
005DBB 7F5DBB 1C 10 E5 02                 bset  StateStatus,SynchLost     ; Set "SynchLost" bit of "StateStatus" variable (bit1)
005DBF 7F5DBF 1D 10 E5 01                 bclr  StateStatus,Synch         ; Clear "Synch " bit of "StateStatus" variable (bit0)
005DC3 7F5DC3 1D 10 E5 04                 bclr  StateStatus,StateNew      ; Clear "StateNew " bit of "StateStatus" variable (bit2)
005DC7 7F5DC7 20 1A                       bra   STATE_STATUS_done         ; Branch to STATE_STATUS_done:
005DC9 7F5DC9                         SynchLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have either just reached one of the four possible lock points, or
                                      ;    we are already in the synch loop.
                                      ;*****************************************************************************************
005DC9 7F5DC9 1C 10 E5 01                 bset  StateStatus,Synch        ; Set "Synch " bit of "StateStatus" variable (bit0)
005DCD 7F5DCD 1C 10 E5 04                 bset  StateStatus,StateNew     ; Set "StateNew" bit of "StateStatus" variable (bit2)
005DD1 7F5DD1 1D 10 E5 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005DD5 7F5DD5 20 0C                       bra   STATE_STATUS_done        ; Branch to STATE_STATUS_done:
005DD7 7F5DD7                         NoLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have the state machine is still looking for a synch lock.
                                      ;*****************************************************************************************
005DD7 7F5DD7 1D 10 E5 01                 bclr  StateStatus,Synch        ; Clear "Synch" bit of "StateStatus" variable (bit0)
005DDB 7F5DDB 1D 10 E5 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005DDF 7F5DDF 1D 10 E5 04                 bclr  StateStatus,StateNew     ; Clear "StateNew" bit of "StateStatus" variable (bit 2)
005DE3 7F5DE3                         STATE_STATUS_done:
                                      ;*****************************************************************************************
                                      ; - Get three consecutive rising edge signals for engine RPM and
                                      ;   calculate the period. This period is for one fifth of a revolution (72 degrees).
                                      ;   RPM, Ignition and  Fuel calculations are done in the main loop.
                                      ;
                                      ; - Also, the tachometer in the instrument cluster of the '95 Dodge Ram with the 8.0L V10
                                      ;   is expecting a 12V square wave signal of 125Hz at 3000 engine RPM. In order to
                                      ;   generate the appropriate signal for this tachometer the following logic is applied:
                                      ;   3000 RPM/60 = 50 Hz
                                      ;   125/50 = 2.5 pulse per revolution, or 5 pulses per 2 revolution
                                      ;   Because we have ten notches on the crank tone ring, if we toggle an output every 3
                                      ;   notches we can create the desired frequency. Then it is just a matter of hardware
                                      ;   conditioning the circuit to create the desired 12V square wave signal
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Reload stall counter with compare value. Stall check is done in the main loop every
                                      ;   mSec. "Stallcnt" is decremented every mSec and reloaded at every crank signal.
                                      ;*****************************************************************************************
005DE3 7F5DE3 18 0B FF 00 17                  movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005DE8 7F5DE8 CD 08 00                    ldy   #veBins_E       ; Load index register Y with address of first configurable
                                                              ; constant on buffer RAM page 1 (vebins)
005DEB 7F5DEB EC EA 03 E6                 ldd   $03E6,Y       ; Load Accu A with value in buffer RAM page 1 offset 998
                                                              ; "Stallcnt" (stall counter)(offset = 998)
005DEF 7F5DEF 7C 10 22                    std  Stallcnt       ; Copy to "Stallcnt" (no crank or stall condition counter)
                                                              ; (1mS increments)
005DF2 7F5DF2 1E 10 24 10 1A              brset ICflgs,Ch1_3d,CAS_3d   ; If "Ch1_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
005DF7 7F5DF7 1E 10 24 04 0D              brset ICflgs,Ch1_On,CAS_2nd  ; If "Ch1_On" bit of "ICflgs" is set, branch to "CAS_2nd:"
005DFC 7F5DFC DC 52                       ldd   ECT_TC1H               ; Load accu D with value in "ECT_TC1H"
005DFE 7F5DFE 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005E01 7F5E01 1C 10 24 04                     bset  ICflgs,Ch1_On          ; Set "Ch1_On" bit of "ICflgs"
005E05 7F5E05 18 20 00 BA                 bra   CASDone                ; Branch to CASDone:
005E09 7F5E09                         CAS_2nd:
005E09 7F5E09 1C 10 24 10                 bset  ICflgs,Ch1_3d          ; Set "Ch1_3d" bit of "ICflgs"
005E0D 7F5E0D 18 20 00 B2                 bra   CASDone                ; Branch to CASDone:
005E11 7F5E11                         CAS_3d:
005E11 7F5E11 DC 52                       ldd   ECT_TC1H          ; Load accu D with value in "ECT_TC1H"
005E13 7F5E13 7C 10 17                    std   CAS2ndtk          ; Copy to "CAS2ntk"
005E16 7F5E16 B3 10 15                    subd  CAS1sttk          ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "CAS2ntk"
                                                                      ; Result in Acu D will be stored as either "CASprd512" or "CASprd256"
                                                                                              ; depending on whether we are cranking or running
005E19 7F5E19 18 04 10 17 10 15               movw  CAS2ndtk,CAS1sttk  ; Copy value in "CAS2ndtk" to "CAS1sttk"
005E1F 7F5E1F 1D 10 24 10                 bclr  ICflgs,Ch1_3d     ; Clear "Ch1_3d" bit of "ICflgs"
                                      ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; - Old Code  ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                                      ;    brset ICflgs,Ch1_2nd,CAS_2nd ; If "Ch1_2nd" bit of "ICflgs" is set, branch to "CAS_2nd:"
                                      ;    brset ICflgs,Ch1_3d,CAS_3d   ; If "Ch1_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
                                      ;    ldd   ECT_TC1H               ; Load accu D with value in "ECT_TC1H"
                                      ;    std   CAS1sttk               ; Copy to "CAS1sttk"
                                      ;    bset  ICflgs,Ch1_2nd         ; Set "Ch1_2nd" bit of "ICflgs"
                                      ;    bra   CASDone                ; Branch to CASDone:
                                      ;CAS_2nd:
                                      ;    ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
                                      ;    std   CAS2ndtk        ; Copy to "CAS2ndtk"
                                      ;    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
                                      ;    std   CASprd1tk       ; Copy result to "CASprd1tk"
                                      ;    bclr  ICflgs,Ch1_2nd  ; Clear "Ch1_2nd" bit of "ICflgs"
                                      ;    bset  ICflgs,Ch1_3d   ; Set "Ch1_3d" bit of "ICflgs"
                                      ;    bra   CASDone         ; Branch to CASDone:
                                      ;CAS_3d:
                                      ;    ldd   ECT_TC1H        ; Load accu D with value in "ECT_TC1H"
                                      ;    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC7H"
                                      ;    std   CASprd2tk       ; Copy result to "CASprd2tk"
                                      ;    addd  CASprd1tk       ; (A:B)+(M:M+1)_->A:B "CASprd2tk" + "CASprd1tk" = "CASprdtk"
                                      ;    bclr  ICflgs,Ch1_3d   ; Clear "Ch1_3d" bit of "ICflgs"
                                      ;
                                      ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                                      ;*****************************************************************************************
                                      ; - All calculations that use the Crank Angle Sensor period need to know what the
                                      ;   resolution is. The timers are initalized with a 5.12uS resoluion but switched to
                                      ;   2.56uS resolution when the engine tranistions from crank mode to run mode.
                                      ;*****************************************************************************************
005E23 7F5E23 1E 10 E1 04 17              brset engine,run,CAS256 ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CAS256:
005E28 7F5E28 7C 10 9A                    std   CASprd512       ; Copy result to "CASprd512" (CAS period in 5.12uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (5.12uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk512")
                                      ;******************************************************************************************
005E2B 7F5E2B CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E2E 7F5E2E 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005E30 7F5E30 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E32 7F5E32 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E35 7F5E35 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
005E36 7F5E36 7C 10 1D                        std   Degx10tk512    ; Copy result to "Degx10tk512"
005E39 7F5E39 18 79 10 1F                     clrw  Degx10tk256    ; Clear "Degx10tk256"
005E3D 7F5E3D 20 15                           bra   CASprdDone     ; Branch to CASprdDone:
005E3F 7F5E3F                         CAS256:
005E3F 7F5E3F 7C 10 9C                    std   CASprd256      ; Copy result to "CASprd256" (CAS period in 2.56uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (2.56uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk256")
                                      ;******************************************************************************************
005E42 7F5E42 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E45 7F5E45 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd256/72)
005E47 7F5E47 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E49 7F5E49 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E4C 7F5E4C 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk256"
005E4D 7F5E4D 7C 10 1F                        std   Degx10tk256    ; Copy result to "Degx10tk256"
005E50 7F5E50 18 79 10 1D                 clrw  Degx10tk512    ; Clear "Degx10tk512"
005E54 7F5E54                         CASprdDone:
                                      ;*****************************************************************************************
                                      ; - Toggle PB4 (Tach out signal)
                                      ;*****************************************************************************************
005E54 7F5E54 96 01                       ldaa  PORTB        ; Load ACC A with value in Port B
005E56 7F5E56 88 10                       eora  #$10         ; Exclusive or with $00010000
005E58 7F5E58 5A 01                       staa   PORTB       ; Copy to Port B (toggle Bit4, Tach out)
                                      ;*****************************************************************************************
                                      ; - Determine if the engine is cranking or running. The timer is initialized with a
                                      ;   5.12uS time base and the engine status bit field "engine" is cleared on power up.
                                      ;   "Spantk" will roll over at ~85 RPM with a 5.12uS base and at ~169 RPM with a
                                      ;   2.56uS base. The time base is switched from 5.12uS to 2.56uS at ~300 RPM which should
                                      ;   be at a speed when the engine is running. Engine speed can drop to as low as ~169 RPM
                                      ;   before ignition calculations cannot be done. It is not likely that the engine will
                                      ;   continue to run at this speed and will stall. Stall detection is done in the main
                                      ;   loop if the period between crank sensor signals is greater than ~2 seconds.
                                      ;*****************************************************************************************
005E5A 7F5E5A 1E 10 E1 04 60              brset  engine,run,CASprdOK ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CASDone:
                                      ;*****************************************************************************************
                                      ; - Look up the cranking RPM limit ("crankingRPM_F")
                                      ;*****************************************************************************************
005E5F 7F5E5F 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005E64 7F5E64 CD 08 00                    ldy   #veBins_E   ; Load index register Y with address of first configurable
                                                            ; constant on buffer RAM page 1 (veBins_E)
005E67 7F5E67 EE EA 03 E2                 ldx   $03E2,Y     ; Load Accu X with value in buffer RAM page 1 (offset 994)($03E2)
                                                            ; ("crankingRPM_F", cranking RPM limit)
                                      ;*****************************************************************************************
                                      ; - Convert to crank angle sensor period ticks in 5.12uS resolution
                                      ;*****************************************************************************************
005E6B 7F5E6B CC C3 46                    ldd  #$C346           ; Load accu D with Lo word of  10 cyl RPMk (5.12uS clock tick)
005E6E 7F5E6E CD 00 23                    ldy  #$0023           ; Load accu Y with Hi word of 10 cyl RPMk (5.12uS clock tick)
005E71 7F5E71 11                          ediv                  ; Extended divide (Y:D)/(X)=>Y;Rem=>D "RPMk" / "crankingRPM_F"
                                                                ; = crank angle sensor period in 5.12uS resolution
                                      ;*****************************************************************************************
                                      ; - Compare the limit period with the current period
                                      ;*****************************************************************************************
005E72 7F5E72 BD 10 9A                    cpy   CASprd512       ; Compare cranking RPM limit period to "CASprd512"
005E75 7F5E75 25 28                           blo   StillCranking   ; Period is greater than that for "crankingRPM_F" so engine is
                                                                    ; still cranking. Branch to StillCranking:
005E77 7F5E77 20 00                       bra   SwitchToRun     ; Branch to SwitchToRun:
005E79 7F5E79                         SwitchToRun:
005E79 7F5E79 18 0B 7F 00 6E              movb #$7F,ECT_PTPSR   ; Load ECT_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005E7E 7F5E7E 18 0B 7F 03 FE              movb #$7F,TIM_PTPSR   ; (TIM_PTPSR equ $03FE) Load TIM_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005E83 7F5E83 1C 10 E1 04                 bset  engine,run      ; Set "run" bit of "engine" variable
005E87 7F5E87 1D 10 E1 02                 bclr  engine,crank    ; Clear "crank" bit of "engine" variable
005E8B 7F5E8B 1D 10 E2 01                 bclr engine2,base512  ; Clear the "base512" bit of "engine" bit field
005E8F 7F5E8F 1C 10 E2 02                 bset engine2,base256  ; Set the "base256" bit of "engine" bit field
005E93 7F5E93 18 79 10 9A                 clrw  CASprd512       ; Clear "CASprd512"
