005C7D 7F5C7D                         ECT_TC2_ISR:
                                      ;*****************************************************************************************
                                      ; - Get two consecutive rising edge signals for vehicle speed and
                                      ;   calculate the period. KPH calculations are done in the main loop
                                      ;*****************************************************************************************
005C7D 7F5C7D 1E 10 24 08 0B              brset ICflgs,Ch2alt,VSS2 ; If "Ch2alt" bit of "ICflgs" is set, branch to "VSS2:"
005C82 7F5C82 DC 54                       ldd  ECT_TC2H            ; Load accu D with value in "ECT_TC2H"
005C84 7F5C84 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005C87 7F5C87 1C 10 24 08                 bset ICflgs,Ch2alt       ; Set "Ch2alt" bit of "ICflgs"
005C8B 7F5C8B 20 10                       bra  ECT2_ISR_Done       ; Branch to "ECT2_ISR_Done:"
005C8D 7F5C8D                         VSS2:
005C8D 7F5C8D DC 54                       ldd  ECT_TC2H       ; Load accu D with value in "ECT_TC2H"
005C8F 7F5C8F B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC2H"
005C92 7F5C92 7C 10 9E                    std  VSSprd         ; Copy result to "VSSprd"
005C95 7F5C95 1D 10 24 08                 bclr ICflgs,Ch2alt  ; Clear "Ch2alt" bit of "ICflgs"
005C99 7F5C99 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005C9D 7F5C9D                         ECT2_ISR_Done:
005C9D 7F5C9D 0B                          rti                ; Return from Interrupt
