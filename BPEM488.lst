006309 7F6309 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
00630F 7F630F 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
006313 7F6313 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
006316 7F6316 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
006319 7F6319 19 EE                       leay D,Y          ; Curve row pointer -> Y
00631B 7F631B 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
