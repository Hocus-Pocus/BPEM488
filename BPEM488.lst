                                      ; - ECT ch2 Interrupt Service Routine (for VSS calculations)
                                      ;*****************************************************************************************
005CCC 7F5CCC                         ECT_TC2_ISR:
                                      ;*****************************************************************************************
                                      ; - Get two consecutive rising edge signals for vehicle speed and
                                      ;   calculate the period. KPH calculations are done in the main loop
                                      ;*****************************************************************************************
005CCC 7F5CCC 1E 10 24 08 0B              brset ICflgs,Ch2alt,VSS2 ; If "Ch2alt" bit of "ICflgs" is set, branch to "VSS2:"
005CD1 7F5CD1 DC 54                       ldd  ECT_TC2H            ; Load accu D with value in "ECT_TC2H"
005CD3 7F5CD3 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005CD6 7F5CD6 1C 10 24 08                 bset ICflgs,Ch2alt       ; Set "Ch2alt" bit of "ICflgs"
005CDA 7F5CDA 20 10                       bra  ECT2_ISR_Done       ; Branch to "ECT2_ISR_Done:"
005CDC 7F5CDC                         VSS2:
005CDC 7F5CDC DC 54                       ldd  ECT_TC2H       ; Load accu D with value in "ECT_TC2H"
005CDE 7F5CDE B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC2H"
005CE1 7F5CE1 7C 10 9E                    std  VSSprd         ; Copy result to "VSSprd"
005CE4 7F5CE4 1D 10 24 08                 bclr ICflgs,Ch2alt  ; Clear "Ch2alt" bit of "ICflgs"
005CE8 7F5CE8 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005CEC 7F5CEC                         ECT2_ISR_Done:
005CEC 7F5CEC 0B                          rti                ; Return from Interrupt
005CED 7F5CED                         ECT_TC3_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT ch3 Interrupt Service Routine for Ign1(1&6) control
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005CED 7F5CED 4C 49 80                    bset ECT_TCTL2,Bit7    ; Clear Ch3 output line to zero on compare
005CF0 7F5CF0 4D 49 40                    bclr ECT_TCTL2,Bit6    ; Clear Ch3 output line to zero on compare
005CF3 7F5CF3 DC 44                       ldd  ECT_TCNTH         ; Contents of Timer Count Register-> Accu D
005CF5 7F5CF5 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time)
005CF8 7F5CF8 5C 56                       std  ECT_TC3H          ; Copy result to Timer IC/OC register 0 (Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005CFA 7F5CFA 0B                          rti                    ; Return from Interrupt
