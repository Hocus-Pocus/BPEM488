0052C5 7F52C5 18 03 01 C0 10 29           movw #$01C0,CrvColOfst  ; 448 -> Offset from the curve page to the curve column (ASE_COR_LU)
                                                                      ; (asePctTable)(actual offset is 896)            (ASE_COR_LU)
0052CB 7F52CB 18 04 10 6C 10 2B           movw Cltx10,CrvCmpVal   ; Engine Coolant Temperature (Degrees F x 10) ->     (ASE_COR_LU)
