005E88 7F5E88                         DoRevCntr:
005E88 7F5E88 B6 10 21                    ldaa  RevCntr        ; Load Accu A with value in "RevCntr"
005E8B 7F5E8B 81 09                       cmpa  #$09           ; Compare with decimal 9
005E8D 7F5E8D 27 24                       beq   CAS1           ; If equal branch to CAS1: (First CAS signal)
005E8F 7F5E8F 81 08                       cmpa  #$08           ; Compare with decimal 8
005E91 7F5E91 27 30                       beq   CAS2           ; If equal branch to CAS2: (Second CAS signal)
005E93 7F5E93 81 07                       cmpa  #$07           ; Compare with decimal 7
005E95 7F5E95 27 31                       beq   CAS3           ; If equal branch to CAS3: (Third CAS signal)
005E97 7F5E97 81 06                       cmpa  #$06           ; Compare with decimal 6
005E99 7F5E99 27 32                       beq   CAS4           ; If equal branch to CAS4: (Forth CAS signal)
005E9B 7F5E9B 81 05                       cmpa  #$05           ; Compare with decimal 5
005E9D 7F5E9D 27 33                       beq   CAS5           ; If equal branch to CAS5: (Fifth CAS signal)
005E9F 7F5E9F 81 04                       cmpa  #$04           ; Compare with decimal 4
005EA1 7F5EA1 27 34                       beq   CAS6           ; If equal branch to CAS6: (Sixth CAS signal)
005EA3 7F5EA3 81 03                       cmpa  #$03           ; Compare with decimal 3
005EA5 7F5EA5 27 35                       beq   CAS7           ; If equal branch to CAS7: (Seventh CAS signal)
005EA7 7F5EA7 81 02                       cmpa  #$02           ; Compare with decimal 2
005EA9 7F5EA9 27 36                       beq   CAS8           ; If equal branch to CAS8: (Eighth CAS signal)
005EAB 7F5EAB 81 01                       cmpa  #$01           ; Compare with decimal 1
005EAD 7F5EAD 27 37                       beq   CAS9           ; If equal branch to CAS9: (Nineth CAS signal)
005EAF 7F5EAF 81 00                       cmpa  #$00           ; Compare with zero
005EB1 7F5EB1 27 38                       beq   CAS10          ; If equal branch to CAS10: (Tenth CAS signal)
