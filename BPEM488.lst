                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
006231 7F6231                         ReEntCrvPos:
006231 7F6231 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
006234 7F6234 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
00623A 7F623A 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
00623E 7F623E FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
006241 7F6241 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
006244 7F6244 19 EE                       leay D,Y          ; Curve row pointer -> Y
006246 7F6246 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
                                                            ; (holds the contents of the incremented row bin)
                                      ;*****************************************************************************************
                                      ; - CrvRowLo, CrvCmpVal and CrvRowHi are all positive. CrvCmpVal is the greater than CrvRowLo.
                                      ;   Now see if CrvRowHi is greater than CrvCmpVal. If it is, we have the index number,
                                      ;   if it is not, loop back to increment to the next bin and check again.
                                      ;*****************************************************************************************
00624B 7F624B FE 10 2F                    ldx  CrvRowHi     ; Curve row high boundary -> X
00624E 7F624E BE 10 2B                    cpx  CrvCmpVal    ; Compare curve curve row high boundary with curve compare value
006251 7F6251 24 08                       bhs  GotNumPos    ; If contents of incremented row bin is greater than or equal to
                                                            ; curve compareson value then branch to GotNumPos:
006253 7F6253 B6 10 2E                    ldaa IndexNum     ; Incremented position in the row or column of the curve comparison
                                                            ; value for interpolation -> A
006256 7F6256 B1 10 2D                    cmpa CrvBinCnt    ; Compare Incremented position in the row or column of the curve
                                                            ; comparison value for interpolation with number of bins in the curve
                                                            ; row or column minus 1
006259 7F6259 26 D6                       bne  ReEntCrvPos  ; If (A)-(M) if IndexNum does not = CrvBinCnt then branch to
                                                            ; ReEntCrvPos:
00625B 7F625B                         GotNumPos:
                                      ;*****************************************************************************************
