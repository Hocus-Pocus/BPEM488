005D63 7F5D63 CE 6E 7E                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005D66 7F5D66 F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005D69 7F5D69 58                          aslb                    ; Shift Accu B 1 place to the left
005D6A 7F5D6A CA 00                       orab   #$00             ; Bit wise inclusive OR Accu B with 0
005D6C 7F5D6C A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005D6E 7F5D6E 7A 10 14                    staa   State            ; Copy to "State"
005D71 7F5D71 0B                          rti                     ; Return from interrupt
