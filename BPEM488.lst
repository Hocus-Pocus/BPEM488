0059D4 7F59D4                         RcvPageID:               ; "rxmode" = 2
0059D4 7F59D4 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL"
0059D6 7F59D6 7A 10 0E                    staa  pageID         ; Copy to "pageID"
0059D9 7F59D9 B6 10 0F                    ldaa  txcmnd         ; Load Accu A with value in "txcmnd"
0059DC 7F59DC 81 03                       cmpa  #$03           ; Compare with decimal 3 ("B")
0059DE 7F59DE 18 27 01 E9                 beq   ModeB2         ; If the Z bit of CCR is set, branch to ModeB2:
0059E2 7F59E2 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059E5 7F59E5 0B                          rti                  ; Return from interrupt
0059E6 7F59E6                         RcvOSmsb:                ; "rxmode" = 3
0059E6 7F59E6 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset MSB)
0059E8 7F59E8 7A 10 0A                    staa  rxoffsetMSB    ; Copy to "rxoffsetMSB"
0059EB 7F59EB 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059EE 7F59EE 0B                          rti                  ; Return from interrupt
0059EF 7F59EF                         RcvOSlsb:                ; "rxmode" = 4
0059EF 7F59EF 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset LSB)
0059F1 7F59F1 7A 10 0B                    staa  rxoffsetLSB    ; Copy to "rxoffsetLSB"
0059F4 7F59F4 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
0059F7 7F59F7 0B                          rti                  ; Return from interrupt
0059F8 7F59F8                         RcvCntmsb:               ; "rxmode" = 5
0059F8 7F59F8 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count MSB)
0059FA 7F59FA 7A 10 06                    staa  txgoalMSB      ; Copy to "txgoalMSB"
0059FD 7F59FD 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A00 7F5A00 0B                          rti                  ; Return from interrupt
005A01 7F5A01                         RcvCntlsb:               ; "rxmode" = 6
005A01 7F5A01 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count LSB)
005A03 7F5A03 7A 10 07                    staa  txgoalLSB      ; Copy to "txgoalLSB"
005A06 7F5A06 B6 10 0F                    ldaa  txcmnd         ; Load Accu A with value in "txcmnd"
005A09 7F5A09 81 01                       cmpa  #$01           ; Compare with decimal 1 ("C")
005A0B 7F5A0B 18 27 01 3C                 beq   ModeC2         ; If the Z bit of CCR is set, branch to ModeC2:
005A0F 7F5A0F 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A12 7F5A12 0B                          rti                  ; Return from interrupt (ready to receive next byte)
005A13 7F5A13                         RcvData:                 ; "rxmode" = 7
                                      ;*****************************************************************************************
                                      ; - If we are here we must be in "W" mode and receiving either one or
                                      ;   two bytes, depending on the byte count.
                                      ;*****************************************************************************************
005A13 7F5A13 FC 10 06                    ldd  txgoalMSB      ; Load double accumulator with value in
                                                              ; "txgoalMSB:txgoalLSB"
005A16 7F5A16 8C 00 02                    cpd  #$0002         ; Compare with decimal 2
005A19 7F5A19 27 05                       beq  RcvDataMSB     ; If equal branch to RcvDataMSB:
005A1B 7F5A1B 8C 00 01                    cpd  #$0001         ; Compare with decimal 1
005A1E 7F5A1E 27 09                       beq  RcvDataLSB     ; If equal branch to RcvDataLSB:
005A20 7F5A20                         RcvDataMSB:
005A20 7F5A20 96 CF                       ldaa  SCI0DRL       ; Load Accu A with value in "SCI0DRL"(data byte)
005A22 7F5A22 7A 10 10                    staa  dataMSB       ; Copy to "dataMSB"
005A25 7F5A25 72 10 0C                    inc   rxmode        ; Increment "rxmode"(continue to next mode)
005A28 7F5A28 0B                          rti                 ; Return from subroutine
005A29 7F5A29                         RcvDataLSB:             ; "rxmode" = 8
005A29 7F5A29 D6 CF                       ldab  SCI0DRL       ; Load Accu B with value in "SCI0DRL"(data byte)
005A2B 7F5A2B 7B 10 11                    stab  dataLSB       ; Copy to "dataLSB"
005A2E 7F5A2E B6 10 0E                    ldaa  pageID        ; Load accu A with value in "pageID"
005A31 7F5A31 81 01                       cmpa  #$01          ; Compare with decimal 1 (send page 1)
005A33 7F5A33 27 0A                       beq   StorePg1      ; If the Z bit of CCR is set, branch to StorePg1:
005A35 7F5A35 81 02                       cmpa  #$02          ; Compare with decimal 2 (send page 2)
005A37 7F5A37 27 4B                       beq   StorePg2      ; If the Z bit of CCR is set, branch to StorePg2:
005A39 7F5A39 81 03                       cmpa  #$03          ; Compare with decimal 3 (send page 3)
005A3B 7F5A3B 18 27 00 86                 beq   StorePg3      ; If the Z bit of CCR is set, branch to StorePg3:
005A3F 7F5A3F                         StorePg1:
