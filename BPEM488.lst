                                      ;***************************************************************************************** (CRANK_PW_CALC)
0052A7 7F52A7 FC 10 C6                    ldd  FDpw        ; "FDpw"-> Accu D                                           (CRANK_PW_CALC)
0052AA 7F52AA F3 10 C0                        addd Deadband    ; (A:B)+(M:M+1)->A:B ("FDpw"+"Deadband"="CrankPW"       (CRANK_PW_CALC)
0052AD 7F52AD 7C 10 C4                        std  CrankPW     ; Result -> "CrankPW" (cranking injector pulsewidth) (mS x 10) (CRANK_PW_CALC)
                                      ;***************************************************************************************** (CRANK_PW_CALC)
                                      ; - Convert the result to timer ticks in 5.12uS resolution                       (CRANK_PW_CALC)
                                      ;***************************************************************************************** (CRANK_PW_CALC)
0052B0 7F52B0 FC 10 C4                    ldd   CrankPW    ; "CrankPW"->Accu D (cranking injector pulsewidth) (mS x 10) (CRANK_PW_CALC)
0052B3 7F52B3 CD 27 10                        ldy   #$2710     ; Load index register Y with decimal 10000 (for integer math) (CRANK_PW_CALC)
0052B6 7F52B6 13                              emul             ;(D)x(Y)=Y:D ("ReqFuel" * "crankcor" )/100) * 10,000)   (CRANK_PW_CALC)
0052B7 7F52B7 CE 02 00                        ldx   #$200      ; Decimal 512 -> Accu X                                 (CRANK_PW_CALC)
0052BA 7F52BA 11                          ediv             ;(Y:D)/(X)=Y;Rem->D                                         (CRANK_PW_CALC)
                                                               ; ((("ReqFuel" * "crankcor" )/100) * 10,000) / 512 = "CrankPWtk" (CRANK_PW_CALC)
0052BB 7F52BB 7D 10 59                    sty   CrankPWtk  ; Copy result to "CrankPWtk" (Cranking pulse width in 5.12uS (CRANK_PW_CALC)
                                                               ; resolution)                                           (CRANK_PW_CALC)
0052BE 7F52BE 7D 10 5F                    sty   InjOCadd2  ; Second injector output compare adder (5.12uS res)         (CRANK_PW_CALC)
