005DBD 7F5DBD 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005DC0 7F5DC0 1C 10 24 04                 bset  ICflgs,Ch1_2nd         ; Set "Ch1_2nd" bit of "ICflgs"
005DC4 7F5DC4 18 20 00 C0                 bra   CASDone                ; Branch to CASDone:
005DC8 7F5DC8                         CAS_2nd:
