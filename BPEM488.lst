                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
005921 7F5921                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
005921 7F5921                         Inc125uS:
005921 7F5921 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
005924 7F5924 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
005927 7F5927 81 08                       cmpa #$08          ; Compare it with decimal 8
005929 7F5929 26 54                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00592B 7F592B                         DomS:
00592B 7F592B 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
00592F 7F592F 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
005932 7F5932 72 10 03                    inc  mS            ; Increment Millisecond counter
005935 7F5935 B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
005938 7F5938 81 64                       cmpa #$64          ; Compare it with decimal 100
00593A 7F593A 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
00593C 7F593C 81 C8                       cmpa #$C8          ; Compare it with decimal 200
00593E 7F593E 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
005940 7F5940 81 FA                       cmpa #$FA          ; Compare it with decimal 250
005942 7F5942 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
005944 7F5944 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
005946 7F5946                         Do100mS:
005946 7F5946 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
00594A 7F594A 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00594C 7F594C                         Do250mS:
00594C 7F594C 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
005950 7F5950 79 10 03                    clr  mS            ; Clear Millisecond counter
005953 7F5953 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
005956 7F5956 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
005959 7F5959 81 02                       cmpa #$02          ; Compare with decimal 2
00595B 7F595B 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
00595D 7F595D 81 04                       cmpa #$04          ; Compare with decimal 4
00595F 7F595F 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
005961 7F5961 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
005963 7F5963                         Do500mS:
005963 7F5963 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
005967 7F5967 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
00596A 7F596A 81 04                       cmpa #$04          ; Compare with decimal 4
00596C 7F596C 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
00596E 7F596E 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
005970 7F5970                         DoSec:
005970 7F5970 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 250 millisecond counter. Increment "secL". Increment "secH" on roll over
                                      ;*****************************************************************************************
005974 7F5974                         IncSec:
005974 7F5974 79 10 04                    clr  mSx250        ; Clear 250 mSec counter
005977 7F5977 72 10 65                    inc  secL          ; Increment "Seconds" Lo byte
00597A 7F597A 26 03                       bne  RTI_ISR_DONE  ; If the Z bit of CCR is clear, branch to RTI_ISR_DONE:
00597C 7F597C 72 10 64                    inc  secH          ; Increment "Seconds" Hi byte
00597F 7F597F                         RTI_ISR_DONE:
00597F 7F597F 4C 37 80                    bset CRGFLG,RTIF   ; Set "RTIF" bit of "CRGFLG" to clear flag
005982 7F5982 0B                          rti                ; Return from interrupt
