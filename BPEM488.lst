005962 7F5962                                                 ORG     RTI_CODE_START, RTI_CODE_START_LIN
                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
005962 7F5962                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
005962 7F5962                         Inc125uS:
005962 7F5962 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
005965 7F5965 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
005968 7F5968 81 08                       cmpa #$08          ; Compare it with decimal 8
00596A 7F596A 26 54                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00596C 7F596C                         DomS:
00596C 7F596C 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
005970 7F5970 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
005973 7F5973 72 10 03                    inc  mS            ; Increment Millisecond counter
005976 7F5976 B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
005979 7F5979 81 64                       cmpa #$64          ; Compare it with decimal 100
00597B 7F597B 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
00597D 7F597D 81 C8                       cmpa #$C8          ; Compare it with decimal 200
00597F 7F597F 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
005981 7F5981 81 FA                       cmpa #$FA          ; Compare it with decimal 250
005983 7F5983 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
005985 7F5985 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
005987 7F5987                         Do100mS:
005987 7F5987 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
00598B 7F598B 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00598D 7F598D                         Do250mS:
00598D 7F598D 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
005991 7F5991 79 10 03                    clr  mS            ; Clear Millisecond counter
005994 7F5994 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
005997 7F5997 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
00599A 7F599A 81 02                       cmpa #$02          ; Compare with decimal 2
00599C 7F599C 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
00599E 7F599E 81 04                       cmpa #$04          ; Compare with decimal 4
0059A0 7F59A0 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
0059A2 7F59A2 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0059A4 7F59A4                         Do500mS:
0059A4 7F59A4 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
0059A8 7F59A8 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0059AB 7F59AB 81 04                       cmpa #$04          ; Compare with decimal 4
0059AD 7F59AD 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
0059AF 7F59AF 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
0059B1 7F59B1                         DoSec:
0059B1 7F59B1 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 250 millisecond counter. Increment "secL". Increment "secH" on roll over
                                      ;*****************************************************************************************
0059B5 7F59B5                         IncSec:
0059B5 7F59B5 79 10 04                    clr  mSx250        ; Clear 250 mSec counter
0059B8 7F59B8 72 10 65                    inc  secL          ; Increment "Seconds" Lo byte
0059BB 7F59BB 26 03                       bne  RTI_ISR_DONE  ; If the Z bit of CCR is clear, branch to RTI_ISR_DONE:
0059BD 7F59BD 72 10 64                    inc  secH          ; Increment "Seconds" Hi byte
0059C0 7F59C0                         RTI_ISR_DONE:
0059C0 7F59C0 4C 37 80                    bset CRGFLG,RTIF   ; Set "RTIF" bit of "CRGFLG" to clear flag
0059C3 7F59C3 0B                          rti                ; Return from interrupt
                                      ;*****************************************************************************************
0059C4 7F59C4 -> $59C4                RTI_CODE_END            EQU     *     ; * Represents the current value of the paged
                                                                    ; program counter
0059C4 7F59C4 -> $7F59C4              RTI_CODE_END_LIN        EQU     @     ; @ Represents the current value of the linear
                                                                    ; program counter
